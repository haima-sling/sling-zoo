{
  "name": "zoo-management-system",
  "version": "1.0.0",
  "description": "A comprehensive zoo management system with animal tracking, visitor management, and facility monitoring",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write src/ tests/",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "deploy": "node scripts/deploy.js"
  },
  "keywords": [
    "zoo",
    "management",
    "animals",
    "tracking",
    "visitor-management",
    "facility-monitoring"
  ],
  "author": "Zoo Management Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "socket.io": "^4.7.2",
    "redis": "^4.6.7",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "html-webpack-plugin": "^5.5.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zoo-management/zoo-system.git"
  },
  "bugs": {
    "url": "https://github.com/zoo-management/zoo-system/issues"
  },
  "homepage": "https://github.com/zoo-management/zoo-system#readme"
}
